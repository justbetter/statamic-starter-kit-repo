{
    "name": "justbetter/statamic-starter-kit",
    "require": {
        "justbetter/statamic-starter-kit": "@dev"
    },
    "extra": {
        "statamic": {
            "name": "JustBetter Starter Kit",
            "description": "A Statamic starter kit for new projects that use Statamic"
        },
        "laravel": {
            "providers": [
                "JustBetter\\StatamicStarterKit\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Kevin Meijer",
            "email": "kevin@justbetter.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "package"
        }
    ],
    "autoload": {
        "psr-4": {
            "JustBetter\\StatamicStarterKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
